{{> fileHeader }}

protocol ComponentsProtocol {
{{#each components}}
  var {{name}}: {{attributes.customOptions.type}} { get }
{{/each}}
}

protocol TokensProtocol {
{{#each tokens}}
  var {{name}}: {{attributes.customOptions.type}} { get }
{{/each}}
}

protocol ThemeProtocol {
  var tokens: TokensProtocol
  var components: ComponentsProtocol
}
