{{> fileHeader }}

struct {{themeName}}Theme: Theme {
  let tokens: TokensProtocol = {{themeName}}Tokens()
  let components: ComponentsProtocol = {{themeName}}Components()
}

struct {{themeName}}Tokens: Tokens {
{{#each tokens}}
  let {{name}}{{#if attributes.customOptions.includeType}}: {{attributes.customOptions.type}}{{/if}} = {{value}}
{{/each}}
}

struct {{themeName}}Components: Components {
{{#each components}}
  let {{name}}{{#if attributes.customOptions.includeType}}: {{attributes.customOptions.type}}{{/if}} = {{value}}
{{/each}}
}
