{
  "extends": [
    "config:base"
  ],
  "labels": ["dependencies", "renovate"],
  "lockFileMaintenance": true,
  "masterIssue": true,
  "masterIssueAutoclose": true,
  "packageRules": [
    {
      "groupName": "snyk package",
      "packagePatterns": ["snyk"],
      "rangeStrategy": "replace"
    }
  ],
  "semanticCommits": true,
  "unicodeEmoji": true,
  "updateLockFiles": true,
  "vulnerabilityAlerts": {
    "enabled": true
  }
}
